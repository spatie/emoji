{
    "name": "spatie/emoji",
    "description": "Display emoji characters",
    "keywords": [
        "spatie",
        "emoji"
    ],
    "homepage": "https://github.com/spatie/emoji",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "ext-iconv": "*",
        "ext-json": "*",
        "symfony/console": "^6.2",
        "twig/twig": "^2.7",
        "guzzlehttp/guzzle": "^7.0",
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Emoji\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Emoji\\Generator\\": "generator",
            "Spatie\\Emoji\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "generate": "php ./generate generate"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}